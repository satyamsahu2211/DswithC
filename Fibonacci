#include <stdio.h>

int main() {
int n,a=0,b=1,i,c;
printf("enter the number of ");
scanf("%d",&n);
printf("%d\t%d",a,b);

for(i=3;i<=n;i++){
    c=a+b;
    a=b;
    b=c;
    printf("%d",c);
}
printf("enter the number",c);
    return 0;
}
int fibonacci(int);
int main()
 {
     int n,i=0,c;
     scanf("%d",&n);
     printf("fibonacci series\n");
     for(c=i;c<=n;c++){
         printf("%d\n",fibonacci(i));
         i++;
     }return 0;
 }
 int fibonacci(int n)
 {
     if(n==0){
         return 0;
     }
     else if(n==1){
       return 1;
}    else {         return(fibonacci(n-1)+fibonacci(n-2));
    }    }
    
