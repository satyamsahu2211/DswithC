#include <stdio.h>

 void main() {
int n,i,fact=1;
scanf("%d",&n);
for(i=1;i<n;i++)
{
    fact = fact*i;
    
}
printf("factorial =%d\n",fact);
}
int fibonacci(int);
int main()
{
    int n,i=0,c;
    scanf("%d",&n);
    printf("fibonacci series\n");
    for(c=i;c<=n;c++){
        printf("%d\n",fibonacci(i));
        i++;
    }return 0;
}
int fibonacci(int n)
{
    if(n==0){
        return 0;
    }
    else if(n==1){
        return 1;
    }
    else {
        return(fibonacci(n-1)+fibonacci(n-2));
    }
    }
    
